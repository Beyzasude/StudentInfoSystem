@using ModelLayer.Student
@model  StudentResult
@{
    ViewBag.Title = "StudentInfo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Öğrenci Bilgileri</h2>
<table class="table table-bordered">
    <tr>
        <th>AD :</th>
        <td>@Model.Name</td>
    </tr>

    <tr>
        <th>SOYAD :</th>
        <td>@Model.Surname</td>
    </tr>

    <tr>
        <th>EMAİL :</th>
        <td>@Model.Email</td>
    </tr>

    <tr>
        <th>TELEFON :</th>
        <td>@Model.Phone</td>
    </tr>

    <tr>
        <th>ADRES :</th>
        <td>@Model.Address</td>
    </tr>

    <tr>
        <th>TC :</th>
        <td>@Model.Tc</td>
    </tr>

    <tr>
        <th>BÖLÜM :</th>
        <td>@Model.DepartmentName</td>
    </tr>

    <tr>
        <th>DÖNEM :</th>
        <td>@Model.Semester</td>
    </tr>


</table>



